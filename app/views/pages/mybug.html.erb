<h1> <%= current_user.name %> bugs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Screenshot</th>
      <th>BugType</th>
      <th>Status</th>
      <th>Project</th>
      <th>Developer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mybugs.each do |bug| %>
      <tr>
        <td><%= bug.title %></td>
        <td><%= bug.description %></td>
        <td><%= bug.screenshot %></td>
        <td><%= bug.bugtype %></td>
        <td><%= bug.status %></td>
        <td><%= bug.project.name %></td>
        <td><%= bug.user.name %></td>
        <td><%= link_to 'Edit', edit_bug_path(bug) %></td>
      </tr>
    <% end %>
  </tbody>
</table>