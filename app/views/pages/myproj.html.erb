  
<h1><%= current_user.name %> Projects</h1>
<% if @myprojs.any? %>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @myprojs.each do |project| %>
        <tr>
          <td><%= project.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% else %> You have no assigned Projects!

<% end %>