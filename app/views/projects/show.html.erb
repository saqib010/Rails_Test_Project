<h1><%= @projectName %> Bugs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Screenshot</th>
      <th>BugType</th>
      <th>Status</th>
      <th>Developer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |bug| %>
      <tr>
        <td><%= bug.title %></td>
        <td><%= bug.description %></td>
        <td><%= bug.screenshot %></td>
        <td><%= bug.bugtype %></td>
        <td><%= bug.status %></td>
        <td><%= bug.user.name %></td></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<br>
<%=link_to '<button type="button">Back</button>'.html_safe, projects_path%>
